<table class="table table-striped table-hover">
<thead>
<tr>
<th>#</th>
<th>Module</th>
<th>Score</th>
</thead>
</tr>
<tbody>
<% userScore.module_scores.map((moduleScore, i)=> { %>
<tr>
    <td>
    <%= i+1 %>
    </td>
    <td>
    <%= moduleScore.name %>
    </td>
    <td>
    <%= moduleScore.score %>
    </td>
</tr>
<% }) %>
</tbody>
</table>